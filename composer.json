{
  "name": "sun-asterisk/laravel-chatwork",
  "description": "Chatwork SDK for Laravel",
  "type": "library",
  "require": {
    "php": ">=7.2",
    "sun-asterisk/chatwork-php": "^0.1.1",
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SunAsterisk\\Chatwork\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SunAsterisk\\Chatwork\\Laravel\\ServiceProvider"
      ],
      "aliases": {
        "Chatwork": "SunAsterisk\\Chatwork\\Laravel\\Facade"
      }
    }
  }
}
